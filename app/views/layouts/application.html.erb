<!DOCTYPE html>
<html>
  <head>
    <title>Manazon</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= stylesheet_link_tag 'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
   <% if logged_in? %>
    <script type="text/javascript">
      window.currentUser =
        <%= render("api/users/user.json.jbuilder", user: current_user).html_safe %>
    </script>
   <% end %>
    <script type="text/javascript">
        window.logo = "<%= image_url('invertlogo1.png') %>"
        window.bg = "<%= image_url('muscle.png') %>"
        window.mag = "<%= image_url('mag.png') %>"
        window.cart = "<%= image_url('cart.png') %>"
        window.manazonLogo = "<%= image_url('Manazonlogo2.png') %>"
        window.pin = "<%= image_url('pin-removebg-preview.png') %>"
        window.swipeIcons = "<%= image_url('swipeicon.png') %>"
        window.dumbbells = "<%= image_url('dumbbells.png') %>"
        window.ad = "<%= image_url('amazonad.png') %>"
        window.primelogo = "<%= image_url('primelogo.png') %>"
    </script>
  </head>

  <body>
    <%= yield %>
  </body>
</html>
